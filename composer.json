{
    "name": "ovflowd/vamonos",
    "description": "Make encounters with an ease of spanish.",
    "keywords": ["uber", "google", "places", "mobility", "accessibility"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "5.6.*",
        "vlucas/phpdotenv": "~2.2",
        "laravel/socialite": "^2.0",
        "mlntn/lumen-artisan-serve": "~1",
        "alexpechkarev/google-maps": "1.0.9",
        "irazasyed/larasupport": "^1.6"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
